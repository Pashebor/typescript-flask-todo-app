{
  "compilerOptions": {
    "allowUnreachableCode": true,
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "jsx": "react",
    "strictNullChecks": true,
    "sourceMap": true,
    "types": [
      "node"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "filesGlob":[
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}